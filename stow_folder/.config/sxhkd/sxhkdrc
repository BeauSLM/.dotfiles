# terminal
# super + Return
#     $TERMINAL

# reload sxhkd settings
super + shift + s
    pkill -USR1 sxhkd

super + shift + b
    $BROWSER

ctrl + alt + d
    discord

ctrl + shift + Escape
    $TERMINAL -e btop

ctrl + alt + Escape
    $TERMINAL -e nvtop

# Selected area screenshot
super + shift + s
    maim -s | xclip -selection clipboard -t image/png

super + c
    emacsclient -nc

XF86MonBrightnessUp
    xbacklight -inc 5%

XF86MonBrightnessDown
    xbacklight -dec 5%

