# terminal
super + Return
    $TERMINAL

#Browser
super + w
    $BROWSER

#Xkill
super + Escape
    xkill

#reload sxhkd:
super + shift + s
	pkill -USR1 -x sxhkd

#Discord
ctrl + alt + d
    discord

#File-Manager
ctrl + alt + t
     thunar

#File manager
ctrl + alt + f
    $TERMINAL -e ranger

#Nitrogen
ctrl + alt + i
     nitrogen

#btop
ctrl + shift + Escape
    $TERMINAL -e btop

# Selected area screenshot
super + s
    maim -s | xclip -selection clipboard -t image/png

#Brightness up
#XF86MonBrightnessUp
#    xbacklight -inc 10

#Brightness down
#XF86MonBrightnessDown
#    xbacklight -dec 10

