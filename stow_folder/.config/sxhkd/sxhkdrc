# terminal
# super + Return
#     $TERMINAL

#Browser
super + b
    $BROWSER

#Discord
ctrl + alt + d
    discord

#File manager
ctrl + alt + f
    $TERMINAL -e ranger

#Nitrogen
ctrl + alt + i
     nitrogen

#btop
ctrl + shift + Escape
    $TERMINAL -e btop

# Selected area screenshot
super + shift + s
    maim -s | xclip -selection clipboard -t image/png

#Brightness up
XF86MonBrightnessUp
    xbacklight -inc 5%

#Brightness down
XF86MonBrightnessDown
    xbacklight -dec 5%

