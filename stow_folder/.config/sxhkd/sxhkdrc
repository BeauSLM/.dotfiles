# terminal
# super + Return
#     $TERMINAL

super + shift + b
    $BROWSER

ctrl + alt + d
    discord

ctrl + shift + Escape
    $TERMINAL -e btop

ctrl + alt + Escape
    $TERMINAL -e nvtop

# Selected area screenshot
super + shift + s
    maim -s | xclip -selection clipboard -t image/png

#Brightness up
XF86MonBrightnessUp
    xbacklight -inc 5%

#Brightness down
XF86MonBrightnessDown
    xbacklight -dec 5%

